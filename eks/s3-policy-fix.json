{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ReadAccessToDataiesbBuckets",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:HeadObject",
                "s3:ListBucket",
                "s3:GetBucketLocation"
            ],
            "Resource": [
                "arn:aws:s3:::dataiesb",
                "arn:aws:s3:::dataiesb/*",
                "arn:aws:s3:::dataiesb-reports",
                "arn:aws:s3:::dataiesb-reports/*"
            ]
        }
    ]
}
